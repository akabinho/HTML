<body>
    <script>
        document.write("Hello World!")
        function verificarAprovado(nota){
            if (nota >= 7) {
                return "Aprovado!";
            }else{
                return "Reprovado!";
            }
        }
        console.log(verificarAprovado(8));
        console.log(verificarAprovado(5));
    </script>

</body>

</html>


